{
  "name": "@nwx/cfg",
  "version": "0.0.6",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neekware/nwx-cfg.git"
  },
  "keywords": ["ng", "angular", "config", "configuration"],
  "author": "Val Neekman",
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neekware/nwx-cfg/issues"
  },
  "homepage": "https://github.com/neekware/nwx-cfg",
  "scripts": {
    "### Integration ###": "Integration app commands",
    "int:serve": "ng serve web",
    "int:build": "ng build --prod web",
    "int:test": "ng test web --watch=false",
    "int:e2e": "ng e2e web-e2e",
    "int:test:ci": "ng test web --browsers ChromeHeadless --watch=false --code-coverage",
    "int:e2e:ci": "ng e2e web-e2e --protractor-config=apps/web/e2e/protractor.conf.ci.js",
    "int:lint": "ng lint web",
    "### Module ###": "Cfg module commands",
    "pkg:build": "ng build cfg --prod",
    "pkg:test": "ng test cfg --watch=false",
    "pkg:test:ci": "ng test cfg --browsers ChromeHeadless --watch=false --code-coverage",
    "pkg:lint": "ng lint cfg",
    "### Tools ###": "Tool commands",
    "format":
      "node node_modules/.bin/prettier --single-quote --print-width 90 --write 'apps/**/src/*.{ts,css,scss,json}' 'pkgs/**/src/*.{ts,css,scss,json}'",
    "release": "ts-node ./tools/release.ts -- "
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.6.1",
    "@angular-devkit/build-ng-packagr": "~0.6.1",
    "@angular/cli": "~6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "commander": "^2.15.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lodash": "^4.17.10",
    "ng-packagr": "^3.0.0-rc.2",
    "prettier": "^1.12.1",
    "protractor": "~5.3.0",
    "semver": "^5.5.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
